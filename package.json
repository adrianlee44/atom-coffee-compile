{
  "name": "coffee-compile",
  "main": "./lib/main",
  "version": "0.30.0",
  "private": true,
  "description": "Preview, compile and/or save CoffeeScript 1 or 2 in editor to Javascript",
  "keywords": [
    "preview",
    "compile",
    "coffeescript",
    "javascript"
  ],
  "repository": "git@github.com:adrianlee44/atom-coffee-compile.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.9.0"
  },
  "dependencies": {
    "coffee-script": "1.12.7",
    "coffeescript": "2.3.0",
    "resolve": "^1.1.7",
    "season": "^6.0.2"
  },
  "consumedServices": {
    "coffee-compile.provider": {
      "description": "A package for compiling coffeescript",
      "versions": {
        "1.0.0": "registerProviders"
      }
    }
  },
  "configSchema": {
    "split": {
      "type": "string",
      "default": "Right",
      "title": "Split pane direction",
      "enum": [
        "Right",
        "Left",
        "Down",
        "Up"
      ]
    },
    "noTopLevelFunctionWrapper": {
      "type": "boolean",
      "title": "Disable top-level function wrapper",
      "description": "Compile without top-level function safety wrapper",
      "default": true
    },
    "focusEditorAfterCompile": {
      "type": "boolean",
      "description": "Keep focus on original editor when showing preview pane",
      "default": false
    },
    "coffeescriptVersion": {
      "type": "string",
      "default": "1.12.7",
      "title": "Coffeescript version to compile with",
      "enum": [
        "1.12.7",
        "2.0.2"
      ]
    }
  }
}
